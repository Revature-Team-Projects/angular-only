pipeline {
    agent any 
    stages {
        stage('Build') {
          steps {
                sh '''
                cd /home/ec2-user/.jenkins/workspace/ang_clone/angularTest
                ng build
                '''
          }
        }
        stage('Deploy'){
            steps{
                sh '''
                cd /home/ec2-user/.jenkins/workspace/ang_clone/angularTest
                 aws s3 cp --recursive ./dist s3://ui-core/angular
                '''
            }
        }
    }
}